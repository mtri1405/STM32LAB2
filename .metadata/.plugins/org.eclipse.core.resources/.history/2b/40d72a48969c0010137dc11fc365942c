#include "stm32f103c6.h"
#include "software_timer.h"

void blinkLED() {
	HAL_GPIO_TogglePin(LED_RED_GPIO_Port, LED_RED_Pin);
}

void blinkLED_TIM(int duration) {
	if (timerLED_flag == 1) {
		setTimerLED(duration);
		blinkLED();
	}
}
